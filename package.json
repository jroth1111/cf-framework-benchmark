{
  "name": "cf-benchmark",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "deploy": "node scripts/deploy-bench.mjs",
    "test": "pnpm -r test",
    "test:dataset": "pnpm -C packages/dataset exec node src/test.mjs",
    "test:contracts": "pnpm -C bench exec node contract-tests.mjs",
    "smoke": "pnpm -C bench exec node smoke.mjs",
    "bench": "pnpm -C bench exec node src/run.mjs",
    "bench:smoke": "pnpm -C bench exec node src/run.mjs --iterations 1",
    "bench:remote": "pnpm -C bench exec node bench-remote.mjs"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
